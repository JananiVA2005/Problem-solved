import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        float h = scan.nextFloat();
        scan.nextLine();
        String s = scan.nextLine().trim();
        System.out.printf("%s is %d years old and %.2f meters tall.", s, a, h);
    }
}

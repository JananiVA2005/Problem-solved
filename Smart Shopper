public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int cost1 = scan.nextInt();
        int quantity1 = scan.nextInt();
       int cost2 = scan.nextInt();
        int quantity2 = scan.nextInt();
        int cost3 = scan.nextInt();
        int quantity3 = scan.nextInt();
       int budget = scan.nextInt();
      double total = ((cost1*quantity1)+(cost2*quantity2)+(cost3*quantity3));
        System.out.printf("Total (Before Tax): %.2f\n",total);
        
        double tax = total*0.10;
        double bill=total+tax;
        System.out.printf("Total (After Tax): %.2f\n",bill);
        if(bill<=budget){
            System.out.println("Within Budget");
        }else{
            System.out.println("Over Budget");
        }
                    
    }
}

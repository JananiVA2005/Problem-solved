public class Solution {

    public static void main(String[] args) {
       
        Scanner scan = new Scanner(System.in);
        int n =Math.abs(scan.nextInt());
        int max=0;
        int min =0;
        while(n!=0){
            int value =n%10;
            if(value>max){
                max = value;
            }
            else{
                min =value;
            }
            n/=10;
        }System.out.println(max + " " + min);
        
    }
    
}

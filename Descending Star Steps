public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int r =scan.nextInt();
        for(int i=r;i>=0;i--){
            for(int j=0;j<i;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

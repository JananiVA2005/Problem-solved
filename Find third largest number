int[] arr= {12, 45, 67, 23, 89, 90, 55};
		int max=0,sec=0,third=-1;
		for(int i=0;i<arr.length;i++) {
				if(arr[i]>max) 
				{
				third=sec;
				  sec=max;
					max=arr[i];
				
				}
				else if(arr[i]>sec && arr[i]!=max) {
					third=sec;
					sec=arr[i];
				}
				else if(arr[i]>third && arr[i]!=sec && arr[i]!=max) {
					third=arr[i];
				}
			}
		System.out.println(third);
		}}

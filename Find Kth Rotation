class Solution {
    int findKRotation(int arr[]) {
        int n = arr.length;
        int s = 0, e = n - 1;
        while (s < e) {
            int mid = (s + e) / 2;
            if (arr[mid] > arr[e]) {
                s = mid + 1;
            } else {
                e = mid;
            }
        }
        return s;
    }
}

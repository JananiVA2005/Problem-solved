public class school {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();
        int sum =0;
        char[] arr=new char[n];
        for(int i=0;i<n;i++) {
        	arr[i] =scan.next().charAt(0);
        	int ascii =(int)arr[i];
        	int index =i+1;
        	int prod =ascii*index;
        
        if(index%2!=0 || ascii%2 !=0 ) {
       
    	   sum+=prod;
       }}
       System.out.println(sum);
    }}

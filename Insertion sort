public class school{
	 public static int[] insertion(int[] arr,int n) {
		
		for(int i=0;i<n-1;i++) {	
			for(int j=i+1;j>0;j--) {
				if(arr[j-1]>arr[j]) {
					int temp=arr[j];
					arr[j]=arr[j-1];
					arr[j-1]=temp;
					
				}
				else break;
			
				
		
			}}
		return arr;
	 }
	
	public static void main(String[] args) {
	   int[] arr = {5,3,2,1,4};
			
			
			int n =arr.length;
			int[] ans = insertion(arr,n);
			System.out.println(Arrays.toString(ans));
					
				
				
			}}		

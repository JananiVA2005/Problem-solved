public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int sum =0;
        int[] arr = new int[n];
        for(int i=0;i<n;i++){
            arr[i] = scan.nextInt();
            sum+=arr[i];
        }
            int value =sum/n;
        
            System.out.println(value);
        
    }
}
